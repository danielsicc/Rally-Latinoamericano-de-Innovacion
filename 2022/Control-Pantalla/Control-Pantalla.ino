void setup() {
  Serial.begin(115200);
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }

  delay(2000); // Pause for 2 seconds
 
  // Clear the buffer.
  display.clearDisplay();
  display.display();
  // Draw bitmap on the screen

  pluma.attach(13);
  pluma.writeMicroseconds(1000);
}

void loop() {
  riesgo='\0';
  riesgo=Serial.read();
  switch(riesgo){
        case 'a':
          display.clearDisplay();
          display.drawBitmap(0, 0, senialUno, 128, 64, 1);
          display.display();
          pluma.writeMicroseconds(2000);
        break;
        case 'b':
          display.clearDisplay();
          display.drawBitmap(0, 0, senialDos, 128, 64, 1);
          display.display();
          pluma.writeMicroseconds(2000);
        break;
        case 'c':
          display.clearDisplay();
          display.drawBitmap(0, 0, senialTres, 128, 64, 1);
          display.display();
          pluma.writeMicroseconds(1000);
        break;
        case 'd':
          display.clearDisplay();
          display.drawBitmap(0, 0, senialCuatro, 128, 64, 1);
          display.display();
          pluma.writeMicroseconds(1000);
        break;
      }
}
